package Trail;

public class CompareTo {

	public static void main(String[] args) {
		String s1 = "Hell";
		String s2 = "Hello";
		int result =s1.compareTo(s2);
		System.out.println(result);
		/*this gives 0
		 * when the both strings are equal 0 will occur
		 * if first string is less than second string then */
	}

}
