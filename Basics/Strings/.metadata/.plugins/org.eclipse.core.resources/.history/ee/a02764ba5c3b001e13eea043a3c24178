package Trail;

public class ContentEquals {

	public static void main(String[] args) {
		String s1 = "Hello";
		System.out.println(s1.contentEquals("Hello"));//true
		System.out.println(s1.contentEquals("Hel"));//false
		System.out.println(s1.contentEquals("He"));//false
		/*it will check if the sstring contains the equal value or not 
		 * if it is equal then it qill return true
		 * if it is not equal then it will return false*/
	}

}
